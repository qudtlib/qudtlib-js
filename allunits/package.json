{
  "name": "@qudtlib/allunits",
  "version": "6.1.0",
  "description": "Package containing all QUDT units, quantityKinds, and prefixes",
  "main": "dist/cjs/units.js",
  "module": "dist/mjs/units.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/units.js",
      "require": "./dist/cjs/units.js"
    }
